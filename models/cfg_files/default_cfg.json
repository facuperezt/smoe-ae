{
    "img2block": {
        "block_size": 16,
        "img_size": 512
    },

    "ae": {
        "n_kernels": 4,
        "block_size": 16,
        "img_size": 512,
        "n_channels": 1,
        "network_architecture": {
            "conv": {
                "channel_sizes": [8, 16, 32, 64, 128, 256, 512, 1024],
                "add_batchnorm": true,
                "dropout": 0.35
            },
            "lin": {
                "feature_sizes": [1024, 512, 256, 128, 128, 128],
                "add_batchnorm": true,
                "dropout": 0.35
            },
            "smoe": {
                "feature_sizes": [64, 32],
                "add_batchnorm": false,
                "dropout": 0.35
            },
            "combiner": {
                "feature_sizes": [64, 32],
                "add_batchnorm": false,
                "dropout": 0.35
            }
        }
    },

    "positional_encoding": {
        "channels": 1
    },
    "global_mean_optimizer": {
        "n_kernels": 4,
        "block_size": 16,
        "img_size": 512,
        "n_channels": 1,
            "network_architecture": null
        },

    "smoe": {
        "n_kernels": 4,
        "block_size": 16
    },

    "block2img": {
        "block_size": 16,
        "img_size": 512
    },

    "loss_function": {
        "loss_type": "l1_l2",
        "alpha": 0.8,
        "beta": 0.7
    },

    "blockwise_loss_function": {
        "loss_type": "l1_l2",
        "alpha": 0.8,
        "beta": 0.7,
        "ssim_args": {
            "data_range": 1.0
        },
        "ms_ssim_args": {
            "data_range": 1.0
        }
    }
}
